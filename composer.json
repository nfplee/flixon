{
    "name": "flixon/flixon",
    "type": "library",
    "description": "The Flixon Framework.",
    "keywords": ["framework", "flixon"],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "doctrine/annotations": "1.13.*",
        "fpdo/fluentpdo": "1.1.*",
        "mtdowling/cron-expression": "1.1.*",
        "symfony/config": "3.0.*",
        "symfony/event-dispatcher": "3.0.*",
        "symfony/http-foundation": "3.0.*",
        "symfony/http-kernel": "3.0.*",
        "symfony/routing": "3.0.*",
        "wixel/gump": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.*"
    },
    "autoload": {
        "psr-4": {
            "Flixon\\": "src/Flixon/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flixon\\Tests\\": "test/"
        }
    }
}